<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            style="@style/helvetica_neue_bold_16_black_padding0"
            android:id="@+id/tv_popular_keyword_rank"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="italic"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv_popularity_keyword"/>

        <TextView
            android:id="@+id/tv_popularity_keyword"
            style="@style/noto_sans_kr_regular_14_black_padding0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_popular_keyword_rank"
            app:layout_constraintStart_toEndOf="@+id/tv_popular_keyword_rank"
            app:layout_constraintTop_toTopOf="@+id/tv_popular_keyword_rank" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
